<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".UploadMainActivity">

    <Button
        android:id="@+id/main_upload_new_video"
        android:layout_margin="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Video"
        />

    <Button
        android:id="@+id/main_upload_last_video"
        android:layout_margin="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upload Last Video"
        />

    <Button
        android:id="@+id/main_play_last_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Play Last Video"
        />

</LinearLayout>
